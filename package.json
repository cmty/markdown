{
  "name": "@dimerapp/markdown",
  "version": "3.2.0",
  "description": "Convert Markdown to HTML and JSON",
  "main": "index.js",
  "files": [
    "src",
    "index.js",
    "github.json"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "commit": "git-cz",
    "lint": "standard"
  },
  "author": "virk",
  "license": "MIT",
  "dependencies": {
    "detab": "^2.0.1",
    "mdast-util-definitions": "^1.2.3",
    "mdast-util-to-string": "^1.0.5",
    "mdast-util-toc": "^2.1.0",
    "rehype-minify-whitespace": "^2.0.3",
    "rehype-sanitize": "^2.0.1",
    "rehype-sort-attribute-values": "^1.0.1",
    "rehype-sort-attributes": "^1.0.1",
    "rehype-stringify": "^4.0.0",
    "remark-autolink-headings": "git+https://github.com/thetutlage/remark-autolink-headings.git",
    "remark-emoji": "2.0.2",
    "remark-macro": "^1.0.7",
    "remark-parse": "^5.0.0",
    "remark-ping": "2.1.6",
    "remark-rehype": "^3.0.1",
    "remark-slug": "^5.1.0",
    "remark-squeeze-paragraphs": "^3.0.2",
    "unified": "^7.0.0",
    "unist-builder": "^1.0.3",
    "unist-util-visit": "^1.4.0"
  },
  "devDependencies": {
    "@adonisjs/mrm-preset": "^1.0.14",
    "commitizen": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "dedent": "^0.7.0",
    "fs-extra": "^7.0.0",
    "japa": "^2.0.4",
    "japa-cli": "^1.0.1",
    "js-beautify": "^1.8.6",
    "mrm": "^1.2.1",
    "pkg-ok": "^2.3.1",
    "standard": "^12.0.1",
    "vfile": "^3.0.0",
    "yorkie": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "nyc": {
    "exclude": [
      "test"
    ]
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimerapp/markdown.git"
  },
  "keywords": [
    "markdown",
    "dimer"
  ],
  "bugs": {
    "url": "https://github.com/dimerapp/markdown/issues"
  },
  "homepage": "https://github.com/dimerapp/markdown#readme",
  "gitHooks": {
    "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
  }
}
